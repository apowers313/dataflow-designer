{
  "name": "@dataflow-designer/dataflow-file",
  "version": "0.0.1",
  "description": "File source and sink for dataflow",
  "engines": {
    "node": ">=18"
  },
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "test": "ts-mocha -p ./tsconfig.json test/**/*.test.ts",
    "docs": "typedoc",
    "build": "tsc -p ./tsconfig.json",
    "lint": "npx eslint --quiet lib/**/*.ts"
  },
  "author": "Adam Powers <apowers@ato.ms>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "typedoc": {
    "entryPoint": "./index.ts",
    "readmeFile": "./README.md",
    "displayName": "Dataflow File"
  },
  "dependencies": {
    "@dataflow-designer/dataflow-core": "*",
    "@dataflow-designer/stupendous-parser": "*"
  }
}
