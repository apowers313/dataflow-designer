{
  "name": "@dataflow-designer/dataflow-graphql",
  "version": "0.0.1",
  "description": "A GraphQL source for dataflow",
  "engines": {
    "node": ">=18"
  },
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "test": "ts-mocha -p ./tsconfig.json test/**/*.test.ts",
    "docs": "typedoc",
    "build": "tsc -p ./tsconfig.json",
    "lint": "npx eslint --quiet lib/**/*.ts"
  },
  "author": "Adam Powers <apowers@ato.ms>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "typedoc": {
    "entryPoint": "./index.ts",
    "readmeFile": "./README.md",
    "displayName": "Dataflow Graphql"
  },
  "dependencies": {
    "@dataflow-designer/dataflow-core": "*",
    "graphql-request": "^5.0.0",
    "jsonata": "^1.8.6"
  }
}
